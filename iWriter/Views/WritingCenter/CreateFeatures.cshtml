@model iWriter.ViewModels.FeatureViewModels.CreateFeatureViewModel

<h4>Create Features</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateFeatures">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
